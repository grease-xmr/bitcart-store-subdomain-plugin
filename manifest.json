{
  "$schema": "https://bitcart.ai/schemas/plugin/1.3.0/plugin.schema.json",
  "name": "store-subdomains",
  "author": "cj",
  "version": "1.0.0",
  "description": "Lets users define a short name (slug) for their stores which will be accessible via {slug}.mybitcart.ai.",
  "constraints": { "bitcart": "" },
  "installs": [
    { "type": "backend", "path": "src/backend/store-subdomains" },
    { "type": "docker", "path": "src/docker/store-subdomains" },
    { "type": "admin", "path": "src/admin/store-subdomains" }
  ]
}
